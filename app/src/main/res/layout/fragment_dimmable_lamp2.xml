<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="phuong.example.com.androidproject.DimmableLamp2">


    <SeekBar
        android:layout_width="255dp"
        android:layout_height="wrap_content"
        android:id="@+id/seekBar"
        android:layout_gravity="center"
        android:background="#a43737" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Power"
        android:id="@+id/textView2"
        android:layout_gravity="center_horizontal|bottom"
        android:textStyle="bold|italic"
        android:textColor="#fc0505"
        android:textSize="@dimen/abc_action_bar_stacked_max_height" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="273dp"
        android:text="@string/lamp_2"
        android:textColor="#d76262"
        android:textColorHighlight="#ad4040"
        android:textSize="@dimen/abc_action_bar_content_inset_material"
        android:textStyle="bold|italic"
        android:theme="@style/AppTheme" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="247dp"
        app:srcCompat="@drawable/light"
        android:id="@+id/imageView8" />

</FrameLayout>
